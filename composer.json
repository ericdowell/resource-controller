{
    "name": "ericdowell/resource-controller",
    "description": "Resource controllers to match the laravel resource route method.",
    "keywords": ["laravel-package", "resource", "resource-controller", "laravel", "laravel-5", "laravel-controller"],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Dowell",
            "email": "eric@ericdowell.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.5"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "EricDowell\\ResourceController\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/database/factories"
        ],
        "psr-4": {
            "EricDowell\\ResourceController\\Tests\\": "tests/src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
